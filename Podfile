  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  

  # Pods for Barliman iOS Prototype

  target 'Barliman iOS PrototypeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Barliman iOS PrototypeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
